{
    "name": "@devmend/que-ro",
    "version": "0.1.2",
    "description": "A modern queue routing library for Cloudflare Workers",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "vitest",
        "test:ci": "vitest run",
        "lint": "eslint src tests --ext .ts",
        "lint:fix": "eslint src tests --ext .ts --fix",
        "format": "prettier --write src tests",
        "format:check": "prettier --check src tests",
        "type-check": "tsc --noEmit",
        "prepublishOnly": "npm run build && npm run test:ci && npm run lint",
        "changeset": "changeset",
        "version-packages": "changeset version",
        "release": "npm run prepublishOnly && changeset publish"
    },
    "keywords": [
        "cloudflare",
        "worker",
        "queue",
        "routing",
        "typescript",
        "edge"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@changesets/cli": "^2.29.4",
        "@cloudflare/workers-types": "^4.20240512.0",
        "@eslint/js": "^9.0.0",
        "@types/node": "^20.11.30",
        "eslint": "^9.0.0",
        "prettier": "^3.2.5",
        "tsup": "^8.0.2",
        "tsx": "^4.7.2",
        "typescript": "^5.4.3",
        "typescript-eslint": "^7.4.0",
        "vitest": "^1.4.0",
        "vitest-environment-miniflare": "^2.14.4"
    },
    "peerDependencies": {
        "@cloudflare/workers-types": "^4.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "directories": {
        "example": "examples",
        "test": "tests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/DeVmend/QueRo.git"
    },
    "bugs": {
        "url": "https://github.com/DeVmend/QueRo/issues"
    },
    "homepage": "https://github.com/DeVmend/QueRo#readme"
}